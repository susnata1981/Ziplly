<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>zipllydevel</application>
  <version>coupon-feature1</version>

  <!--
    Allows App Engine to send multiple requests to one instance in parallel:
  -->
  <threadsafe>true</threadsafe>
  
  <!-- Configure serving/caching of GWT files -->
  <static-files>
    <include path="**" />

    <!-- The following line requires App Engine 1.3.2 SDK -->
    <include path="**.nocache.*" expiration="0s" />

    <include path="**.cache.*" expiration="365d" />
    <exclude path="**.gwt.rpc" />
  </static-files>

  <!-- Configure java.util.logging -->
  <system-properties>
    <property name="app.environment" value="DEVEL" />
    <property name="gcs_bucket_name" value="ziplly_images" />
    
    <property name="app.mail.endpoint" value="/ziplly/sendmail" />
    <property name="app.upload.endpoint" value="/ziplly/upload" />
        
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties" />
    
    <property name="app.admin.email2" value="zipplyrocks@appspot.gserviceaccount.com" />
    
    <property name="app.admin.email" value="admin@ziplly.com" />
    
    <property name="app.email.general.enable" value="true" />
    <property name="app.email.admin.enable" value="true" />
    
    <property name="app.dev.redirecturl" value="http://127.0.0.1:8888/Ziplly.html?gwt.codesvr=127.0.0.1:9997" />
    
    <!-- Enable/Disable tweet limit based on payment plan for businesses -->
    <property name="app.feature.enablePaymentPlan" value="false" />
	
	<!-- Require email verification -->
    <property name="app.feature.requireEmailVerification" value="true" />
    
    <!-- Restrict registraiton to invitation based -->
    <property name="app.feature.enableRestrictedRegistration" value="false" />
    
    <property name="backend_instance_name_1" value="nagraj-1" />
    
    <!-- Message display time in msec -->
    <property name="app.feature.displayMessageTime" value="5000" />
    
    <!-- Complete the coupon purchase on first callback -->
    <property name="app.feature.completeCouponPurchaseOnFirstCallBack" value="false" />
  </system-properties>

  
  <use-google-connector-j>true</use-google-connector-j>
  <sessions-enabled>true</sessions-enabled>
  
  <!-- Enabling incoming email -->
  <inbound-services>
  	<service>mail</service>
  </inbound-services>

  <warmup-requests-enabled>
      true
  </warmup-requests-enabled>
  
<!-- 
    It's possible to reduce request latency by configuring your application to
    asynchronously write HTTP session data to the datastore:

      <async-session-persistence enabled="true" />
  <use-google-connector-j>true</use-google-connector-j>

    With this feature enabled, there is a very small chance your app will see
    stale session data. For details, see
    http://code.google.com/appengine/docs/java/config/appconfig.html#Enabling_Sessions
  -->

</appengine-web-app>
