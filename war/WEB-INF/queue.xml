<queue-entries>
    <total-storage-limit>10M</total-storage-limit>
    <queue>
        <name>emailqueue</name>
        <rate>4/s</rate>
        <bucket-size>10</bucket-size>
        <max-concurrent-requests>5</max-concurrent-requests>
        <retry-parameters>
            <min-backoff-seconds>5</min-backoff-seconds>
            <max-backoff-seconds>100</max-backoff-seconds>
            <max-doublings>0</max-doublings>
        </retry-parameters>
    </queue>
</queue-entries>