<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:com.github.gwtbootstrap.client.ui" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	</ui:style>
	<g:HTMLPanel>
		<b:Modal ui:field="modal">
			<g:HTMLPanel>
				<b:Alert ui:field="message" close="false" />
				<b:Alert type="INFO" close="false">
					Sorry, we couldn't find any
					neighborhood matching your address.
					Choose from the following list or add it if you don't find it.
				</b:Alert>
				<g:HTMLPanel ui:field="neighborhoodInfoPanel">
				    
				</g:HTMLPanel>
				<i>

				</i>
				<b:Image ui:field="neighborhoodLoadingImage" />
				<b:ListBox ui:field="neighborhoodListBox">

				</b:ListBox>
				<p>
					<i>
						You can add your neighborhood if you don't see it in the above
						list.
					</i>
				</p>
				<b:Accordion>
					<b:AccordionGroup heading="Add Neighborhood">
						<b:ControlGroup ui:field="neighborhoodNameCg">
							<b:Controls>
								<b:Label>Neighborhood Name</b:Label>
								<br />
								<b:TextBox ui:field="neighborhoodNameTextBox"
									placeholder="neighborhood name..." />
								<b:HelpInline ui:field="neighborhoodNameError"></b:HelpInline>
							</b:Controls>
						</b:ControlGroup>
					</b:AccordionGroup>
				</b:Accordion>
				<b:Button type="PRIMARY" ui:field="submitBtn">Submit</b:Button>
				<b:Button ui:field="cancelBtn">No thanks</b:Button>
			</g:HTMLPanel>
		</b:Modal>
	</g:HTMLPanel>
</ui:UiBinder> 